   <Box sx={{ width: 200 }}>\n      <Stack spacing={2} direction=\"row\" sx={{ mb: 1 }} alignItems=\"center\">\n        <VolumeDown />\n        <Slider aria-label=\"Volume\" value={value} onChange={handleChange} />\n        <VolumeUp />\n      </Stack>\n      <Slider disabled defaultValue={30} aria-label=\"Disabled slider\" />\n    </Box>\n  );\n}\n",jsxPreview:'<Stack spacing={2} direction="row" sx={{ mb: 1 }} alignItems="center">\n  <VolumeDown />\n  <Slider aria-label="Volume" value={value} onChange={handleChange} />\n  <VolumeUp />\n</Stack>\n<Slider disabled defaultValue={30} aria-label="Disabled slider" />',moduleTS:"./ContinuousSlider.js",rawTS:"import * as React from 'react';\nimport Box from '@mui/material/Box';\nimport Stack from '@mui/material/Stack';\nimport Slider from '@mui/material/Slider';\nimport VolumeDown from '@mui/icons-material/VolumeDown';\nimport VolumeUp from '@mui/icons-material/VolumeUp';\n\nexport default function ContinuousSlider() {\n  const [value, setValue] = React.useState<number>(30);\n\n  const handleChange = (event: Event, newValue: number | number[]) => {\n    setValue(newValue as number);\n  };\n\n  return (\n    <Box sx={{ width: 200 }}>\n      <Stack spacing={2} direction=\"row\" sx={{ mb: 1 }} alignItems=\"center\">\n        <VolumeDown />\n        <Slider aria-label=\"Volume\" value={value} onChange={handleChange} />\n        <VolumeUp />\n      </Stack>\n      <Slider disabled defaultValue={30} aria-label=\"Disabled slider\" />\n    </Box>\n  );\n}\n"},"SliderSizes.js":{module:"./SliderSizes.js",raw:'import * as React from \'react\';\nimport Box from \'@mui/material/Box\';\nimport Slider from \'@mui/material/Slider\';\n\nexport default function SliderSizes() {\n  return (\n    <Box width={300}>\n      <Slider\n        size="small"\n        defaultValue={70}\n        aria-label="Small"\n        valueLabelDisplay="auto"\n      />\n      <Slider defaultValue={50} aria-label="Default" valueLabelDisplay="auto" />\n    </Box>\n  );\n}\n',jsxPreview:'<Slider\n  size="small"\n  defaultValue={70}\n  aria-label="Small"\n  valueLabelDisplay="auto"\n/>\n<Slider defaultValue={50} aria-label="Default" valueLabelDisplay="auto" />',moduleTS:"./SliderSizes.js",rawTS:'import * as React from \'react\';\nimport Box from \'@mui/material/Box\';\nimport Slider from \'@mui/material/Slider\';\n\nexport default function SliderSizes() {\n  return (\n    <Box width={300}>\n      <Slider\n        size="small"\n        defaultValue={70}\n        aria-label="Small"\n        valueLabelDisplay="auto"\n      />\n      <Slider defaultValue={50} aria-label="Default" valueLabelDisplay="auto" />\n    </Box>\n  );\n}\n'},"DiscreteSlider.js":{module:"./DiscreteSlider.js",raw:"import * as React from 'react';\nimport Box from '@mui/material/Box';\nimport Slider from '@mui/material/Slider';\n\nfunction valuetext(value) {\n  return `${value}\xb0C`;\n}\n\nexport default function DiscreteSlider() {\n  return (\n    <Box sx={{ width: 300 }}>\n      <Slider\n        aria-label=\"Temperature\"\n        defaultValue={30}\n        getAriaValueText={valuetext}\n        valueLabelDisplay=\"auto\"\n        step={10}\n        marks\n        min={10}\n        max={110}\n      />\n      <Slider defaultValue={30} step={10} marks min={10} max={110} disabled />\n    </Box>\n  );\n}\n",jsxPreview:'<Slider\n  aria-label="Temperature"\n  defaultValue={30}\n  getAriaValueText={valuetext}\n  valueLabelDisplay="auto"\n  step={10}\n  marks\n  min={10}\n  max={110}\n/>\n<Slider defaultValue={30} step={10} marks min={10} max={110} disabled />',moduleTS:"./DiscreteSlider.js",rawTS:"import * as React from 'react';\nimport Box from '@mui/material/Box';\nimport Slider from '@mui/material/Slider';\n\nfunction valuetext(value: number) {\n  return `${value}\xb0C`;\n}\n\nexport default function DiscreteSlider() {\n  return (\n    <Box sx={{ width: 300 }}>\n      <Slider\n        aria-label=\"Temperature\"\n        defaultValue={30}\n        getAriaValueText={valuetext}\n        valueLabelDisplay=\"auto\"\n        step={10}\n        marks\n        min={10}\n        max={110}\n      />\n      <Slider defaultValue={30} step={10} marks min={10} max={110} disabled />\n    </Box>\n  );\n}\n"},"DiscreteSliderSteps.js":{module:"./DiscreteSliderSteps.js",raw:"import * as React from 'react';\nimport Box from '@mui/material/Box';\nimport Slider from '@mui/material/Slider';\n\nfunction valuetext(value) {\n  return `${value}\xb0C`;\n}\n\nexport default function DiscreteSliderSteps() {\n  return (\n    <Box sx={{ width: 300 }}>\n      <Slider\n        aria-label=\"Small steps\"\n        defaultValue={0.00000005}\n        getAriaValueText={valuetext}\n        step={0.00000001}\n        marks\n        min={-0.00000005}\n        max={0.0000001}\n        valueLabelDisplay=\"auto\"\n      />\n    </Box>\n  );\n}\n",jsxPreview:'<Slider\n  aria-label="Small steps"\n  defaultValue={0.00000005}\n  getAriaValueText={valuetext}\n  step={0.00000001}\n  marks\n  min={-0.00000005}\n  max={0.0000001}\n  valueLabelDisplay="auto"\n/>',moduleTS:"./DiscreteSliderSteps.js",rawTS:"import * as React from 'react';\nimport Box from '@mui/material/Box';\nimport Slider from '@mui/material/Slider';\n\nfunction valuetext(value: number) {\n  return `${value}\xb0C`;\n}\n\nexport default function DiscreteSliderSteps() {\n  return (\n    <Box sx={{ width: 300 }}>\n      <Slider\n        aria-label=\"Small steps\"\n        defaultValue={0.00000005}\n        getAriaValueText={valuetext}\n        step={0.00000001}\n        marks\n        min={-0.00000005}\n        max={0.0000001}\n        valueLabelDisplay=\"auto\"\n      />\n    </Box>\n  );\n}\n"},"DiscreteSliderMarks.js":{module:"./DiscreteSliderMarks.js",raw:"import * as React from 'react';\nimport Box from '@mui/material/Box';\nimport Slider from '@mui/material/Slider';\n\nconst marks = [\n  {\n    value: 0,\n    label: '0\xb0C',\n  },\n  {\n    value: 20,\n    label: '20\xb0C',\n  },\n  {\n    value: 37,\n    lab